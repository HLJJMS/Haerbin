<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".activity.RegisterCompanyActivity">

    <com.example.haerbin.tools.MyTitleBar
        android:id="@+id/titleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="25dp"
        android:text="五常·无忧办APP"
        android:textColor="#ff333333"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/titleBar" />

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="90dp"
        android:text="企业注册"
        android:textColor="#ff757575"
        android:textSize="29sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/titleBar" />

    <TextView

        android:id="@+id/tv_company_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="55dp"
        android:text="企业名称"
        android:textColor="#ff6b7388"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_title" />

    <EditText
        android:id="@+id/et_company_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="55dp"
        android:background="@color/white"
        android:hint="请输入企业名称"
        android:paddingStart="10dp"
        android:textColorHint="#BCC2D2"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@id/tv_company_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tv_company_name"
        app:layout_constraintTop_toTopOf="@id/tv_company_name" />

    <View
        android:id="@+id/v_company_name"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="55dp"
        android:background="@color/color_e9e9e9"
        app:layout_constraintTop_toBottomOf="@id/et_company_name" />

    <TextView
        android:id="@+id/tv_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="20dp"
        android:text="统一社会信用代码 "
        android:textColor="#ff6b7388"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/v_company_name" />

    <EditText
        android:id="@+id/et_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="55dp"
        android:background="@color/white"
        android:hint="统一社会信用代码"
        android:inputType="textPassword"
        android:paddingStart="10dp"
        android:textColorHint="#BCC2D2"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@id/tv_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tv_id"
        app:layout_constraintTop_toTopOf="@id/tv_id" />

    <View
        android:id="@+id/v_id"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="55dp"
        android:background="@color/color_e9e9e9"
        app:layout_constraintTop_toBottomOf="@id/et_id" />


    <TextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="20dp"
        android:text="法人代表 "
        android:textColor="#ff6b7388"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/v_id" />

    <EditText
        android:id="@+id/et_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="55dp"
        android:background="@color/white"
        android:hint="请输入法人代表"
        android:inputType="textPassword"
        android:paddingStart="10dp"
        android:textColorHint="#BCC2D2"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@id/tv_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tv_name"
        app:layout_constraintTop_toTopOf="@id/tv_name" />

    <View
        android:id="@+id/v_phone"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="55dp"
        android:background="@color/color_e9e9e9"
        app:layout_constraintTop_toBottomOf="@id/et_name" />

    <TextView
        android:id="@+id/tv_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="20dp"
        android:text="法人身份证号 "
        android:textColor="#ff6b7388"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/v_phone" />

    <EditText
        android:id="@+id/et_code"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:hint="请输入法人身份证号"
        android:inputType="textPassword"
        android:paddingStart="10dp"
        android:textColorHint="#BCC2D2"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@id/tv_code"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="55dp"
        app:layout_constraintStart_toEndOf="@id/tv_code"
        app:layout_constraintTop_toTopOf="@id/tv_code" />

    <View
        android:id="@+id/v_code"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="55dp"
        android:background="@color/color_e9e9e9"
        app:layout_constraintTop_toBottomOf="@id/et_code" />

    <TextView
        android:id="@+id/tv_ok"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="55dp"
        android:background="@color/color_2BA4D9"
        android:gravity="center"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:text="提交"
        android:textColor="@color/white"
        app:layout_constraintTop_toBottomOf="@id/v_code" />
</androidx.constraintlayout.widget.ConstraintLayout>